#include <iostream>
#include <string>
using namespace std;

class Artist
{
  public:
    int user_id;
    string username;

  void add_user(int id, string name)
  {
    user_id = id;
    username = name;
  }
  void show_user()
  {
    cout << user_id << endl;
    cout << username;
  }
};
int main() {
    Artist Anik;
    Anik.add_user(105, "raihan");
    Anik.show_user();
    return 0;
}
